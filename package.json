  "bugs": {
    "url": "https://github.com/ryokohbato/makikomi-alert/issues"
  },
  "homepage": "https://github.com/ryokohbato/makikomi-alert#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "destyle.css": "^1.0.15",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-watch": "^5.0.1",
    "jest": "^26.6.1",
    "minimist": "^1.2.5",
    "postcss": "^8.1.1",
    "sass": "^1.27.0",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3",
    "webextension-polyfill-ts": "^0.20.0",
    "webpack": "^5.1.2",
    "webpack-cli": "^4.1.0",
    "webpack-stream": "^6.1.0"
  }
