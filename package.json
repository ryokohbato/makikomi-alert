{
  "name": "makikomi-alert",
  "version": "0.3.2",
  "description": "An extension for Twitter Web App that displays a warning in situations where you will send an unintended reply.",
  "main": "index.js",
  "scripts": {
    "checkVersion": "npx gulp checkVersion",
    "build": "npx gulp build --mode=development",
    "build_script": "npx webpack -c webpack.development.config.js",
    "watch": "npx gulp watch --mode=development",
    "publish": "npx gulp build --mode=production",
    "publish-no-wsl": "npx gulp build --mode=production --no-wsl=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryokohbato/makikomi-alert.git"
  },
